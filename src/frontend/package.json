    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.4",
    "reactstrap": "^9.2.3",
    "sass": "^1.93.2"
